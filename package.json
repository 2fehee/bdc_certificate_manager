  "description": "bdc-certificate-manager with typescript",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server/index.js | pino-pretty",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "lint": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet",
    "lint:fix": "tsc --noEmit && eslint -c .eslintrc.js '{server, test}/*.{js,ts,tsx}' --quiet --fix",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "@openzeppelin/upgrades": "^2.8.0",
    "bignumber.js": "^9.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "latest",
    "dotenv": "^8.2.0",
    "ethereumjs-tx": "^1.3.7",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.9.5",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.7.0",
    "mariadb": "^2.3.1",
    "node-yaml": "^4.0.1",
    "pino": "^5.17.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.6",
    "sequelize-typescript": "^1.1.0",
    "solc": "^0.6.4",
    "truffle": "^5.1.18",
    "uuid": "^7.0.3",
    "web3": "^1.2.6",
    "web3-core": "^1.3.0",
    "multer": "^1.4.2",
    "crypto": "^1.0.1"
